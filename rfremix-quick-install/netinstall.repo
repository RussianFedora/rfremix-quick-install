[russianfedora]
name=RFRemix $releasever - $basearch
failovermethod=priority
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=build-russianfedora-$releasever&arch=$basearch
enabled=1
gpgcheck=0

[russianfedora-updates]
name=RFRemix $releasever - $basearch - Updates
failovermethod=priority
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=build-russianfedora-updates-$releasever&arch=$basearch
enabled=1
gpgcheck=0

[rpmfusion-free-russianfedora]
name=RPM Fusion for Fedora $releasever - Free
failovermethod=priority
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=build-rpmfusion-free-$releasever&arch=$basearch
enabled=1
gpgcheck=0

[rpmfusion-free-russianfedora-updates]
name=RPM Fusion for Fedora $releasever - Free - Updates
failovermethod=priority
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=build-rpmfusion-free-updates-$releasever&arch=$basearch
enabled=1
gpgcheck=0

[rpmfusion-nonfree-russianfedora]
name=RPM Fusion for Fedora $releasever - Non-Free
failovermethod=priority
mirrorlist=http://mirrors.rfremix.ru/mirrorlist?repo=build-rpmfusion-nonfree-$releasever&arch=$basearch
enabled=1
gpgcheck=0

[rpmfusion-nonfree-russianfedora-updates]
